{
  "presets": [
    ["@babel/env", { "targets": { "browsers": "last 2 versions" } }], 
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    // Transform ES modules to CommonJS for Jest support.
    // TODO: only do this in test environment
    ["@babel/plugin-transform-modules-commonjs", { "loose": true }]
  ]
}
