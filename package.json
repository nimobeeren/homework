{
  "name": "ts-everything",
  "version": "0.1.0",
  "description": "A template aimed at optimizing developer experience using A template aimed at optimizing developer experience through excellent code completion, unit tests and automation.",
  "repository": "https://github.com/nimobeeren/ts-everything",
  "author": "Nimo Beeren",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "concurrently yarn:build-*",
    "build-client": "parcel build client/src/index.html -d client/dist",
    "build-server": "parcel build server/src/index.ts -d server/dist --target node",
    "check-types": "tsc",
    "clean": "rimraf .cache coverage **/dist *.log",
    "generate": "graphql-codegen",
    "lint": "eslint --ext ts,tsx,js .",
    "serve": "NODE_ENV=production node server/dist",
    "start": "concurrently yarn:start-*",
    "start-client": "parcel client/src/index.html -d client/dist",
    "start-server": "parcel watch server/src/index.ts -d server/dist --target node & node-dev server/dist",
    "test": "jest --config jest.config.json"
  },
  "engines": {
    "node": ">=11.15",
    "yarn": ">=1.19.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@deity/eslint-config-falcon": "^0.1.4",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@graphql-codegen/add": "^1.8.1",
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/typescript": "^1.8.1",
    "@graphql-codegen/typescript-operations": "^1.8.1",
    "@graphql-codegen/typescript-react-apollo": "^1.8.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/koa-static": "^4.0.1",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/supertest": "^2.0.8",
    "babel-jest": "^24.9.0",
    "concurrently": "^4.1.2",
    "emotion-theming": "^10.0.19",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "node-dev": "^4.0.0",
    "parcel": "^1.12.3",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "waait": "^1.0.5"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@hot-loader/react-dom": "^16.9.0",
    "@koa/cors": "^3.0.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server-koa": "^2.9.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "koa": "^2.8.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "react": "^16.10.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.14"
  }
}
