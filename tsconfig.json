{
  "compilerOptions": {
    // We only use the TypeScript compiler for type checking, transpiling is handled by Babel
    "noEmit": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": ["**/src/**/*", "test-utils", "graphql"]
}
